  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/jest": "^24.9.1",
    "jest": "^24.9.0",
    "prettier": "^2.0.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.0",
